{
  "name": "procore-chat",
  "description": "a simple chat app for procore interns",
  "license": "MIT",
  "repository": "https://github.com/changreytang/procore_chat",
  "version": "0.0.1",
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "postinstall": "cd client && npm install",
    "rails-server": "foreman start -f Procfile.dev",
    "build:production:client": "(cd client && npm run build:production:client --silent)",
    "build:client": "(cd client && npm run build:client --silent",
    "build:dev:client": "(cd client && npm run build:dev:client --silent)",
    "test": "rspec"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {},
  "devDependencies": {
    "file-loader": "^0.9.0",
    "image-loader": "0.0.1",
    "url-loader": "^0.5.7"
  }
}
